<footer
  class="footer full-width"
  style="
    --footer-bg: {{ section.settings.background_color }};
    --footer-padding-top: {{ section.settings.padding_top }}px;
    --footer-padding-bottom: {{ section.settings.padding_bottom }}px;
    --footer-section-gap: {{ section.settings.section_gap }}px;
    --logo-width: {{ settings.logo_width }}px;
    --logo-width-mobile: {{ settings.logo_width_mobile }}px;

    --newsletter-bg: {{ section.settings.newsletter_bg_color }};
    --newsletter-image-opacity: {{ section.settings.newsletter_image_opacity | divided_by: 100.0 }};
    --newsletter-padding-desktop: {{ section.settings.newsletter_padding_desktop }}px;
    --newsletter-padding-mobile: {{ section.settings.newsletter_padding_mobile }}px;
    --newsletter-title-color: {{ section.settings.newsletter_title_color }};
    --newsletter-title-size-desktop: {{ section.settings.newsletter_title_size_desktop }}px;
    --newsletter-title-size-mobile: {{ section.settings.newsletter_title_size_mobile }}px;
    --newsletter-title-weight: {{ section.settings.newsletter_title_weight }};
    --newsletter-input-bg: {{ section.settings.newsletter_input_bg }};
    --newsletter-input-color: {{ section.settings.newsletter_input_color }};
    --newsletter-input-size-desktop: {{ section.settings.newsletter_input_size_desktop }}px;
    --newsletter-input-size-mobile: {{ section.settings.newsletter_input_size_mobile }}px;
    --newsletter-button-color: {{ section.settings.newsletter_button_color }};
    --newsletter-button-size-desktop: {{ section.settings.newsletter_button_size_desktop }}px;
    --newsletter-button-size-mobile: {{ section.settings.newsletter_button_size_mobile }}px;
    --newsletter-button-weight: {{ section.settings.newsletter_button_weight }};

    --description-color: {{ section.settings.description_color }};
    --description-size: {{ section.settings.description_size }}px;
    --description-weight: {{ section.settings.description_weight }};

    --menu-heading-color: {{ section.settings.menu_heading_color }};
    --menu-heading-size: {{ section.settings.menu_heading_size }}px;
    --menu-heading-weight: {{ section.settings.menu_heading_weight }};

    --menu-link-color: {{ section.settings.menu_link_color }};
    --menu-link-size: {{ section.settings.menu_link_size }}px;
    --menu-link-weight: {{ section.settings.menu_link_weight }};

    --copyright-color: {{ section.settings.copyright_color }};
    --border-color: {{ section.settings.border_color }};
  "
>
  {%- comment -%} Newsletter Section {%- endcomment -%}
  {%- if section.settings.show_newsletter -%}
    <div class="footer__newsletter">
      {%- if section.settings.newsletter_image -%}
        <div class="footer__newsletter-image-wrap">
          {{ section.settings.newsletter_image | image_url: width: 1200 | image_tag: class: 'footer__newsletter-image', loading: 'lazy' }}
          <div class="footer__newsletter-image-fade"></div>
        </div>
      {%- endif -%}
      <div class="footer__newsletter-inner">
        <h2 class="footer__newsletter-title">
          {{ section.settings.newsletter_title }}
        </h2>
        {%- form 'customer', class: 'footer__newsletter-form' -%}
          <input type="hidden" name="contact[tags]" value="newsletter">
          <div class="footer__newsletter-input-wrap">
            <input
              type="email"
              name="contact[email]"
              class="footer__newsletter-input"
              placeholder="{{ 'footer.newsletter_placeholder' | t }}"
              required
              autocomplete="email"
              aria-label="{{ 'footer.newsletter_placeholder' | t }}"
            >
            <button type="submit" class="footer__newsletter-button">
              {{ section.settings.newsletter_button_text }}
            </button>
          </div>
        {%- endform -%}
      </div>
    </div>
  {%- endif -%}

  {%- comment -%} Main Footer Content {%- endcomment -%}
  <div class="footer__content">
    <div class="footer__main">
      {%- comment -%} Brand Column - Left Side {%- endcomment -%}
      <div class="footer__brand">
        <div class="footer__logo">
          <a href="{{ routes.root_url }}" class="footer__logo-link">
            {%- if settings.logo_dark -%}
              {{ settings.logo_dark | image_url: width: 400 | image_tag: class: 'footer__logo-image', loading: 'lazy' }}
            {%- else -%}
              <span class="footer__logo-text">{{ shop.name }}</span>
            {%- endif -%}
          </a>
        </div>

        {%- if section.settings.brand_description != blank -%}
          <p class="footer__description">
            {{ section.settings.brand_description }}
          </p>
        {%- endif -%}

        <div class="footer__social footer__social--desktop">
          {% render 'social-icons', class: 'footer__social-icons' %}
        </div>
      </div>

      {%- comment -%} Menu Columns - Right Side {%- endcomment -%}
      <div class="footer__menus">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'menu' -%}
              <div class="footer__menu-column" {{ block.shopify_attributes }}>
                {%- comment -%} Desktop Menu {%- endcomment -%}
                <div class="footer__menu-desktop">
                  <h3 class="footer__menu-title">
                    {{ block.settings.menu.title }}
                  </h3>
                  <ul class="footer__menu-list">
                    {%- for link in block.settings.menu.links -%}
                      <li class="footer__menu-item">
                        <a href="{{ link.url }}" class="footer__menu-link">
                          {{ link.title }}
                        </a>
                      </li>
                    {%- endfor -%}
                  </ul>
                </div>

                {%- comment -%} Mobile Accordion {%- endcomment -%}
                <details class="footer__menu-mobile">
                  <summary class="footer__menu-summary">
                    <span class="footer__menu-title">
                      {{ block.settings.menu.title }}
                    </span>
                    <span class="footer__menu-icon">
                      <svg width="12" height="12" viewBox="0 0 12 12" fill="none" stroke="currentColor" stroke-width="1.5">
                        <path d="M3 4.5L6 7.5L9 4.5"/>
                      </svg>
                    </span>
                  </summary>
                  <ul class="footer__menu-list">
                    {%- for link in block.settings.menu.links -%}
                      <li class="footer__menu-item">
                        <a href="{{ link.url }}" class="footer__menu-link">
                          {{ link.title }}
                        </a>
                      </li>
                    {%- endfor -%}
                  </ul>
                </details>
              </div>
          {%- endcase -%}
        {%- endfor -%}
      </div>
    </div>

    {%- comment -%} Mobile Social Icons {%- endcomment -%}
    <div class="footer__social footer__social--mobile">
      {% render 'social-icons', class: 'footer__social-icons' %}
    </div>

    {%- comment -%} Bottom Bar {%- endcomment -%}
    <div class="footer__bottom">
      <div class="footer__copyright">
        &copy; {{ 'now' | date: '%Y' }} {{ shop.name }}. {{ 'footer.all_rights_reserved' | t }}
      </div>

      {%- if section.settings.show_payment_icons -%}
        <div class="footer__payment">
          {%- for type in shop.enabled_payment_types -%}
            {{ type | payment_type_svg_tag }}
          {%- endfor -%}
        </div>
      {%- endif -%}
    </div>
  </div>
</footer>

{% stylesheet %}
  .footer {
    background-color: var(--footer-bg);
    padding-top: var(--footer-padding-top);
    padding-bottom: var(--footer-padding-bottom);
    width: 100%;
  }

  /* Newsletter Section */
  .footer__newsletter {
    background-color: var(--newsletter-bg);
    padding: var(--newsletter-padding-desktop) var(--page-margin);
    margin-bottom: var(--footer-section-gap);
    position: relative;
    overflow: hidden;
  }

  .footer__newsletter-image-wrap {
    position: absolute;
    top: 0;
    left: 0;
    width: 60%;
    height: 100%;
    pointer-events: none;
  }

  .footer__newsletter-image {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: top left;
    opacity: var(--newsletter-image-opacity);
  }

  .footer__newsletter-image-fade {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(
      to right,
      transparent 0%,
      var(--newsletter-bg) 100%
    );
  }

  .footer__newsletter-inner {
    max-width: var(--page-width);
    margin: 0 auto;
    position: relative;
    z-index: 1;
  }

  .footer__newsletter-title {
    font-family: var(--font-heading-family);
    font-size: var(--newsletter-title-size-desktop);
    font-weight: var(--newsletter-title-weight);
    color: var(--newsletter-title-color);
    margin: 0 0 12px;
  }

  .footer__newsletter-form {
    width: 100%;
  }

  .footer__newsletter-input-wrap {
    display: flex;
    align-items: center;
    border-bottom: 1px solid var(--newsletter-input-color);
  }

  .footer__newsletter-input {
    flex: 1;
    padding: 12px 0;
    border: none;
    border-radius: 0;
    background: transparent;
    font-family: var(--font-primary-family);
    font-size: var(--newsletter-input-size-desktop);
    color: var(--newsletter-input-color);
    outline: none;
    -webkit-appearance: none;
    appearance: none;
  }

  .footer__newsletter-input::placeholder {
    color: var(--newsletter-input-color);
    opacity: 0.6;
  }

  .footer__newsletter-button {
    background: transparent;
    border: none;
    border-radius: 0;
    padding: 12px 0 12px 16px;
    font-family: var(--font-primary-family);
    font-size: var(--newsletter-button-size-desktop);
    font-weight: var(--newsletter-button-weight);
    color: var(--newsletter-button-color);
    text-transform: uppercase;
    letter-spacing: 0.08em;
    cursor: pointer;
    white-space: nowrap;
    transition: opacity 0.2s ease;
    -webkit-appearance: none;
    appearance: none;
  }

  .footer__newsletter-button:hover {
    opacity: 0.7;
  }

  /* Footer Content Container */
  .footer__content {
    max-width: var(--page-width);
    margin: 0 auto;
    padding: 0 var(--page-margin);
  }

  /* Main Footer */
  .footer__main {
    display: grid;
    grid-template-columns: 1fr 2fr;
    gap: 60px;
    padding-bottom: var(--footer-section-gap);
  }

  /* Brand Column */
  .footer__brand {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .footer__logo-link {
    display: block;
    text-decoration: none;
  }

  .footer__logo-image {
    display: block;
    width: var(--logo-width);
    height: auto;
  }

  .footer__logo-text {
    font-family: var(--font-heading-family);
    font-size: 24px;
    font-weight: var(--font-heading-weight);
    color: var(--menu-heading-color);
  }

  .footer__description {
    font-size: var(--description-size);
    font-weight: var(--description-weight);
    line-height: 1.6;
    color: var(--description-color);
    max-width: 280px;
    margin: 0;
  }

  /* Social Icons */
  .footer__social--desktop {
    display: block;
  }

  .footer__social--mobile {
    display: none;
  }

  .footer__social-icons {
    display: flex;
    gap: 16px;
  }

  .footer__social-icons a {
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--menu-link-color);
    transition: opacity 0.2s ease;
  }

  .footer__social-icons a:hover {
    opacity: 0.7;
  }

  .footer__social-icons svg {
    width: 20px;
    height: 20px;
  }

  /* Menu Columns */
  .footer__menus {
    display: flex;
    justify-content: flex-end;
    gap: 60px;
  }

  .footer__menu-column {
    min-width: 140px;
  }

  .footer__menu-desktop {
    display: block;
  }

  .footer__menu-mobile {
    display: none;
  }

  .footer__menu-title {
    font-family: var(--font-primary-family);
    font-size: var(--menu-heading-size);
    font-weight: var(--menu-heading-weight);
    color: var(--menu-heading-color);
    text-transform: uppercase;
    letter-spacing: 0.08em;
    margin: 0 0 20px;
  }

  .footer__menu-list {
    list-style: none;
    padding: 0;
    margin: 0;
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .footer__menu-link {
    font-size: var(--menu-link-size);
    font-weight: var(--menu-link-weight);
    color: var(--menu-link-color);
    text-decoration: none;
    transition: opacity 0.2s ease;
  }

  .footer__menu-link:hover {
    opacity: 0.7;
  }

  /* Bottom Bar */
  .footer__bottom {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-top: var(--footer-section-gap);
    border-top: 1px solid var(--border-color);
  }

  .footer__copyright {
    font-size: 13px;
    color: var(--copyright-color);
  }

  .footer__payment {
    display: flex;
    gap: 8px;
  }

  .footer__payment svg {
    height: 24px;
    width: auto;
  }

  /* Mobile Styles */
  @media screen and (max-width: 768px) {
    .footer__newsletter {
      padding: var(--newsletter-padding-mobile) var(--page-margin);
    }

    .footer__newsletter-title {
      font-size: var(--newsletter-title-size-mobile);
    }

    .footer__newsletter-input {
      font-size: var(--newsletter-input-size-mobile);
    }

    .footer__newsletter-button {
      font-size: var(--newsletter-button-size-mobile);
    }

    .footer__main {
      grid-template-columns: 1fr;
      gap: 32px;
    }

    .footer__brand {
      gap: 16px;
    }

    .footer__logo-image {
      width: var(--logo-width-mobile);
    }

    .footer__description {
      max-width: none;
    }

    .footer__social--desktop {
      display: none;
    }

    .footer__social--mobile {
      display: block;
      padding: 24px 0;
      border-top: 1px solid var(--border-color);
    }

    .footer__social--mobile .footer__social-icons {
      justify-content: center;
    }

    /* Mobile Accordion Menus */
    .footer__menus {
      flex-direction: column;
      gap: 0;
    }

    .footer__menu-column {
      border-top: 1px solid var(--border-color);
    }

    .footer__menu-desktop {
      display: none;
    }

    .footer__menu-mobile {
      display: block;
    }

    .footer__menu-summary {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 16px 0;
      cursor: pointer;
      list-style: none;
      color: var(--menu-heading-color);
    }

    .footer__menu-summary::-webkit-details-marker {
      display: none;
    }

    .footer__menu-summary .footer__menu-title {
      margin: 0;
      font-size: 13px;
    }

    .footer__menu-icon {
      transition: transform 0.2s ease;
    }

    .footer__menu-mobile[open] .footer__menu-icon {
      transform: rotate(180deg);
    }

    .footer__menu-mobile .footer__menu-list {
      padding-bottom: 16px;
    }

    .footer__menu-mobile .footer__menu-link {
      text-transform: uppercase;
      font-size: 12px;
      letter-spacing: 0.05em;
    }

    .footer__bottom {
      flex-direction: column;
      gap: 16px;
      text-align: center;
    }
  }
{% endstylesheet %}

{% javascript %}
  // Close other accordions when one opens
  document.querySelectorAll('.footer__menu-mobile').forEach(details => {
    details.addEventListener('toggle', (e) => {
      if (details.open) {
        document.querySelectorAll('.footer__menu-mobile').forEach(other => {
          if (other !== details && other.open) {
            other.open = false;
          }
        });
      }
    });
  });
{% endjavascript %}

{% schema %}
{
  "name": "t:sections.footer.name",
  "class": "footer-section",
  "settings": [
    {
      "type": "header",
      "content": "t:sections.footer.settings.newsletter.header"
    },
    {
      "type": "checkbox",
      "id": "show_newsletter",
      "label": "t:sections.footer.settings.newsletter.show",
      "default": true
    },
    {
      "type": "text",
      "id": "newsletter_title",
      "label": "t:sections.footer.settings.newsletter.title",
      "default": "Join our newsletter"
    },
    {
      "type": "text",
      "id": "newsletter_button_text",
      "label": "t:sections.footer.settings.newsletter.button_text",
      "default": "Join"
    },
    {
      "type": "header",
      "content": "t:sections.footer.settings.newsletter_image.header"
    },
    {
      "type": "image_picker",
      "id": "newsletter_image",
      "label": "t:sections.footer.settings.newsletter_image.image"
    },
    {
      "type": "range",
      "id": "newsletter_image_opacity",
      "label": "t:sections.footer.settings.newsletter_image.opacity",
      "min": 5,
      "max": 100,
      "step": 5,
      "default": 30,
      "unit": "%"
    },
    {
      "type": "header",
      "content": "t:sections.footer.settings.newsletter_style.header_desktop"
    },
    {
      "type": "range",
      "id": "newsletter_padding_desktop",
      "label": "t:sections.footer.settings.newsletter_style.padding",
      "min": 16,
      "max": 80,
      "step": 4,
      "default": 32,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "newsletter_title_size_desktop",
      "label": "t:sections.footer.settings.newsletter_style.title_size",
      "min": 18,
      "max": 48,
      "step": 1,
      "default": 24,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "newsletter_input_size_desktop",
      "label": "t:sections.footer.settings.newsletter_style.input_size",
      "min": 12,
      "max": 20,
      "step": 1,
      "default": 15,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "newsletter_button_size_desktop",
      "label": "t:sections.footer.settings.newsletter_style.button_size",
      "min": 10,
      "max": 18,
      "step": 1,
      "default": 13,
      "unit": "px"
    },
    {
      "type": "header",
      "content": "t:sections.footer.settings.newsletter_style.header_mobile"
    },
    {
      "type": "range",
      "id": "newsletter_padding_mobile",
      "label": "t:sections.footer.settings.newsletter_style.padding",
      "min": 12,
      "max": 60,
      "step": 4,
      "default": 24,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "newsletter_title_size_mobile",
      "label": "t:sections.footer.settings.newsletter_style.title_size",
      "min": 16,
      "max": 32,
      "step": 1,
      "default": 20,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "newsletter_input_size_mobile",
      "label": "t:sections.footer.settings.newsletter_style.input_size",
      "min": 12,
      "max": 18,
      "step": 1,
      "default": 14,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "newsletter_button_size_mobile",
      "label": "t:sections.footer.settings.newsletter_style.button_size",
      "min": 10,
      "max": 16,
      "step": 1,
      "default": 12,
      "unit": "px"
    },
    {
      "type": "header",
      "content": "t:sections.footer.settings.newsletter_colors.header"
    },
    {
      "type": "color",
      "id": "newsletter_bg_color",
      "label": "t:sections.footer.settings.newsletter_colors.background",
      "default": "#D97706"
    },
    {
      "type": "color",
      "id": "newsletter_title_color",
      "label": "t:sections.footer.settings.newsletter_colors.title_color",
      "default": "#FFFFFF"
    },
    {
      "type": "select",
      "id": "newsletter_title_weight",
      "label": "t:sections.footer.settings.newsletter_colors.title_weight",
      "options": [
        { "value": "400", "label": "t:options.font_weight.regular" },
